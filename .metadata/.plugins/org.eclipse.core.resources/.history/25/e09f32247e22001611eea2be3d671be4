import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		System.out.println("Enter a value");
		int x;
		Scanner input = new Scanner(System.in);
        String str = input.nextLine();
        x = Integer.parseInt(str);
		do {
			System.out.println("x=" + x);
			System.out.println("#");
			x++;
		} while (x<6);
	}

}
